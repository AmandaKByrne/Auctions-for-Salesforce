        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Estimated_FMV__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>(Opportunity__r.Amount / Opportunity__r.Number_of_Items__c ) * Quantity__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Automatically calculated fair market value based on the value of the opportunity and the number of item.</inlineHelpText>
        <label>Estimated FMV</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Currency</type>
    </fields>
    <fields>
